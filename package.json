{
  "name": "my-react-sdk",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/my-react-sdk.umd.cjs",
  "module": "./dist/my-react-sdk.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-react-sdk.js",
      "require": "./dist/my-react-sdk.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint:sdk": "eslint src",
    "lint:demo": "eslint demo",
    "typecheck": "tsc --noEmit",
    "build:sdk": "npm run lint:sdk && vite build",
    "build:demo": "npm run lint:demo && BUILD_DEMO=true vite build --mode uat",
    "dev:serve-sdk": "npx serve -l 8080 dist/sdk",
    "dev:serve-demo": "npx serve -l 3001 dist/demo",
    "dev:demo": "vite"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "terser": "^5.44.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.50.1",
    "vite": "^5.0.8",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  }
}
