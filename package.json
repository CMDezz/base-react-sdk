{
  "name": "my-react-sdk",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/my-react-sdk.umd.cjs",
  "module": "./dist/my-react-sdk.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-react-sdk.js",
      "require": "./dist/my-react-sdk.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "serve-sdk": "npx serve -l 8080 dist",
    "dev-react-16": "cd Test/React16 && [ -d node_modules ] || npm install && npx react-scripts start",
    "dev-react-19": "cd Test/React19 && [ -d node_modules ] || npm install && npx react-scripts start",
    "dev-vue": "cd Test/Vue && [ -d node_modules ] || npm install && npm run dev"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vite-plugin-css-injected-by-js": "^3.5.2"
  }
}
